{
  "name": "anubhavbtp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/postgres": "^1.13.0",
    "@sap/cds": "^8",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "[sqlite]": {
        "db": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        }
      },
      "[pg]": {
        "db": {
          "kind": "postgres",
          "impl": "@cap-js/postgres",
          "credentials": {
            "host": "localhost",
            "port": "5433",
            "user": "postgres",
            "password": "admin",
            "database": "postgres"
          }
        }
      }
    }
  }
}
